#!/usr/bin/node

class Square extends require('./4-rectangle.js') {
  constructor (size) {
    super(size, size);
  }

  charPrint (c) {
    if (c === undefined) {
	    this.print();
    } else {
	    for (let i = 0; i < this.height; i++) {
        console.log('C'.repeat(this.width));
	    }
    }
  }
}
module.exports = Square;
