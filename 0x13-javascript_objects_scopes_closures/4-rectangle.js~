#!/usr/bin/node
class Rectangle {
  constructor (w, h) {
    if (w > 0 && h > 0) {
      this.width = w;
      this.height = h;
    }
  }

  print () {
    for (let i = 0; i < this.height; i++) {
      console.log('X'.repeat(this.width));
    }
  }
    rotate() {
	let i = 0;
	if ( i = this.width) {
	    this.width = this.height;
	}
    }
    double () {
	console.log(2 * this.width, 2 * this.height);
    }
}
module.exports = Rectangle;
